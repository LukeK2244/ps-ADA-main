-- Table structure for table `ADA_PROJECT_PREFIX_EVENT_HISTORY`
--

CREATE TABLE IF NOT EXISTS `ADA_PROJECT_PREFIX_EVENT_HISTORY` (
  `ID` int(11) NOT NULL AUTO_INCREMENT,
  `RECORD_ID` int(11) DEFAULT NULL,
  `EVENT_TYPE_ID` int(11) DEFAULT NULL,
  `CREATED_BY` varchar(255) DEFAULT NULL,
  `AUTOMATION_TYPE_ID` int(11) DEFAULT NULL,
  `COMMENT` varchar(4000) DEFAULT NULL,
  `CREATED_ON` datetime(6) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `ADA_PROJECT_PREFIX_EVENT_HISTORY_RECORD_ID_FK` (`RECORD_ID`),
  KEY `ADA_PROJECT_PREFIX_EVENT_HISTORY_EVENT_TYPE_ID_FK` (`EVENT_TYPE_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;

--
-- Constraints for dumped tables
--

--
-- Constraints for table `ADA_PROJECT_PREFIX_EVENT_HISTORY`
--
ALTER TABLE `ADA_PROJECT_PREFIX_EVENT_HISTORY`
  ADD CONSTRAINT `ADA_PROJECT_PREFIX_EVENT_HISTORY_EVENT_TYPE_ID_FK` FOREIGN KEY (`EVENT_TYPE_ID`) REFERENCES `ADA_PROJECT_PREFIX_EVENT_TYPE` (`ID`),
  ADD CONSTRAINT `ADA_PROJECT_PREFIX_EVENT_HISTORY_RECORD_ID_FK` FOREIGN KEY (`RECORD_ID`) REFERENCES `ADA_PROJECT_PREFIX` (`PREFIX_ID`);
COMMIT;
