CREATE TABLE IF NOT EXISTS `ADA_REF_SECTION_HEALTH` (
 `SECTION_HEALTH_ID` int(11) NOT NULL AUTO_INCREMENT,
 `TITLE` varchar(255) DEFAULT NULL,
 `RELATED_SECTION_TYPE_ID` int(11) DEFAULT NULL,
 `RELATED_SECTION_ID` int(11) DEFAULT NULL,
 `TASK_TYPE_ID` int(11) DEFAULT NULL,
 `IS_ACTIVE` bit(1) DEFAULT 1,
 `CREATED_BY` varchar(255) DEFAULT NULL,
 `CREATED_ON` datetime(6) DEFAULT NULL,
 `UPDATED_BY` varchar(255) DEFAULT NULL,
 `UPDATED_ON` datetime(6) DEFAULT NULL,
 PRIMARY KEY (`SECTION_HEALTH_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
