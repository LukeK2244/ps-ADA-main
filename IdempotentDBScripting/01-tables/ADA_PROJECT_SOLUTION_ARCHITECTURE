CREATE TABLE IF NOT EXISTS `ADA_PROJECT_SOLUTION_ARCHITECTURE` (
  `PROJECT_SOLUTION_ARCHITECTURE_ID` int(11) NOT NULL AUTO_INCREMENT,
  `PROJECT_ID` int(11) DEFAULT NULL,
  `OVERVIEW` text DEFAULT NULL,
  `USER_AUTHENTICATION` text DEFAULT NULL,
  `PROJECT_AUTHENTICATION` text DEFAULT NULL,
  `PROJECT_AUTHENTICATION_USERNAMES` text DEFAULT NULL,
  `USER_PROVISIONING_ACCOUNT_CREATION_SYNC` text DEFAULT NULL,
  `USER_PROVISIONING_GROUP_SYNC` text DEFAULT NULL,
  `USER_PROVISIONING_INITIAL_USER_COMMUNITY_SET_UP` text DEFAULT NULL,
  `USER_PROVISIONING_USER_DEACTIVATION` text DEFAULT NULL,
  `DOCUMENT_LIFECYCLE_MANAGEMENT` text DEFAULT NULL,
  `APPLICATION_DATA_ARCHIVAL_STRATEGY` text DEFAULT NULL,
  `INTERNATIONALISATION` text DEFAULT NULL,
  `VPN_CONNECTIONS` text DEFAULT NULL,
  `AWS_PRIVATELINK` text DEFAULT NULL,
  `AUDIT_APPLICATION_LEVEL` text DEFAULT NULL,
  `AUDIT_ACTION_LEVEL` text DEFAULT NULL,
  `AUDIT_FIELD_LEVEL` text DEFAULT NULL,
  `EMAIL_OUTBOUND` text DEFAULT NULL,
  `EMAIL_INBOUND` text DEFAULT NULL,
  `DATA_MIGRATION` text DEFAULT NULL,
  `SOLUTION_ARCHITECTURE_DIAGRAM` int(11) DEFAULT NULL,
  `SAML_FLOW_SOLUTION_DIAGRAM` int(11) DEFAULT NULL,
  `IS_ACTIVE` tinyint(1) DEFAULT 1,
  `CREATED_ON` datetime NOT NULL,
  `CREATED_BY` varchar(255) NOT NULL,
  `UPDATED_ON` datetime NOT NULL,
  `UPDATED_BY` varchar(255) NOT NULL,
  PRIMARY KEY (`PROJECT_SOLUTION_ARCHITECTURE_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_general_ci;
